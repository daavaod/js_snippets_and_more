const uniqueNumbers = [...new Set(numbers)]
// [1,2,3,4,5,6,7]
const uniqueNames = [...new Set(names)]
// ['John', 'Mads', 'Bob']

// unique objects from an array by key
let countriesUnique = Object.values(countries.reduce((acc:any, obj:any) => ({ ...acc, [obj.country]: obj }), {}));

